{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"require": {
		"laravel/framework": "4.2.*",
		"koraktor/steam-condenser": "dev-master",
		"aws/aws-sdk-php-laravel": "1.*",
		"stripe/stripe-php": "v1.9.0",
		"way/generators": "3",
		"vtalbot/markdown": "1.*",
		"thomaswelton/laravel-gravatar": "dev-master",
		"zizaco/entrust": "1.2.x",
		"zizaco/confide": "3.2.x",
		"laravelbook/ardent": "dev-master",
		"thapp/jitimage": "dev-master",
		"jyc/rcon": "dev-default",
		"iron-io/iron_mq": "1.5.*",
		"indatus/dispatcher": "~1.4",
		"ehesp/steam-login": "~1.0.1",
		"syntax/steam-api": "~1.1.0",
		"gries/rcon": "dev-master"
	},
	"require-dev": {
		"fzaninotto/faker": "v1.2.0"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		],
		"psr-0": {
			"Quelle": "app/"
		},
		"files": [
	        "vendor/koraktor/steam-condenser/lib/SteamCondenser/SteamCondenser.php"
	    ]
	},

	"scripts": {
		"post-install-cmd": [
			"php artisan optimize"
		],
		"pre-update-cmd": [
			"php artisan clear-compiled"
		],
		"post-update-cmd": [
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev",
	"prefer-stable" : true
}
